{% extends "PortfolioBundle::base.html.twig" %}

{% block pageTitle %}Błąd 500 {% endblock %}

{% block pageDesc %}Błąd 500 - serwer nie odpowiada.{% endblock %}


{% block pageBody %}
<body class="projekty">

<div class="cover"></div>

<header id="projects_menu errorpage">

    {#{% include "PortfolioBundle::sticky-header.html.twig" %}#}

</header>

<section id="error">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h2> {% trans %}Błąd serwera. Spróbuj później{% endtrans %}</h2>
                {% image 'bundles/portfolio/img/error500.png' %}
                    <img class="img-responsive" src="{{ asset_url }}" alt="Błąd 500"/>
                {% endimage %}
            </div>
        </div>
    </div>

</section>

{% endblock %}


{% block scripts %}

    {% javascripts
        'bundles/portfolio/js/jquery-2.1.3.min.js'
        'bundles/portfolio/js/bootstrap.min.js'
        'bundles/portfolio/js/jquery.scrollTo-1.4.3.1-min.js'
        'bundles/portfolio/js/pace.min.js'
        'bundles/portfolio/js/transformico.js'
        'bundles/portfolio/js/main_script.js'
        output ='assets/js/compiled-projects.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}

{% endblock %}

